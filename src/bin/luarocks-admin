#!/Programs/Lua/5.1.4/bin/lua
package.path = [[/Users/hisham/luarocks/src/?.lua;/Users/hisham/luarocks/src/?/init.lua;]]..package.path

local command_line = require("luarocks.command_line")

local program_name = "luarocks-admin"
local program_description = "LuaRocks repository administration interface"

local commands = {}
commands.help = require("luarocks.help")
commands.make_manifest = require("luarocks.make_manifest")
commands.add = require("luarocks.add")
commands.remove = require("luarocks.admin_remove")
commands.refresh_cache = require("luarocks.refresh_cache")

command_line.run_command(program_name, program_description, commands, ...)
